//doctype
html(lang="ru").no-js
  head
    title Start Page | Gulp + Jade + Stylus Scaffold
    meta(http-equiv="Content-Type", content="text/html; charset=utf-8")
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content='A description of this page')
    link(rel="icon" type="image/png", href="./assets/img/favicon.png")
    link(rel='stylesheet', type='text/css', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css')
    link(rel='stylesheet', type='text/css', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css')
    link(rel='stylesheet', type='text/css', href='https://fonts.googleapis.com/css?family=Open+Sans:700&subset=cyrillic,cyrillic-ext')
    link(rel='stylesheet', type='text/css', href='./assets/css/app.css')

body
  .container
    h2.text-center Start page
    h3.text-center Webpack + Pug + Stylus

    table.table.table-bordered
      tr
        th(width="30%") Link
        th Page name
      if htmlWebpackPlugin.options.pages.length
        each page in htmlWebpackPlugin.options.pages
          tr
            td
              a(href=page.fileName + ".html", target="_blank") #{page.fileName}.html
            td #{page.name}

