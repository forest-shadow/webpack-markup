    style.
      body {
        background: #46aad9;
        color: #fff;
      }

      h3 {
        margin-bottom: 30px;
      }

      a {
        color: #c6f1f8;
        font-weight: bold;
        transition: all .1s;
      }

      a:hover {
        color: #93d2de;
        text-decoration: none;
      }

      .footer.navbar-fixed-bottom {
        height: 230px;
        padding-bottom: 20px;
      }

      .footer.navbar-fixed-bottom svg {
        display: block;
        margin: 0 auto;
      }

      .footer.navbar-fixed-bottom svg path {
        fill: #23556c;
        stroke: none;
        transition: all .2s ease-in;
      }

      .footer.navbar-fixed-bottom svg:hover path {
        fill: #2a6682;
        stroke: none;
      }
